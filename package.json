{
  "name": "highland-esmodule",
  "version": "2.13.5",
  "description": "Re-publication of highland.js as an esmodule.",
  "module": "lib/module.mjs",
  "scripts": {
    "init": "rimraf lib && mkdirp lib",
    "prepare": "npm-run-all init --parallel build:*",
    "build:bundle.cjs": "browserify node_modules/highland/lib/index.js -o lib/bundle.cjs",
    "build:module": "./generate-module.mjs > lib/module.mjs"
  },
  "keywords": [
    "highland",
    "highlandjs",
    "esm",
    "esmodule",
    "module"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "highland": "=2.13.5"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
